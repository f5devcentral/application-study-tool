          "pluginVersion": "4.8.2",
          "queryType": "logs",
          "rawSql": "SELECT Timestamp as \"timestamp\", concat(Body, ' ', LogAttributes) as \"body\", SeverityText as \"level\", TraceId as \"traceID\", mapConcat(LogAttributes, ResourceAttributes) AS labels FROM \"otel\".\"otel_logs\" WHERE ( timestamp >= $__fromTime AND timestamp <= $__toTime ) ORDER BY timestamp DESC LIMIT 1000",
